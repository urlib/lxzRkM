</div><script type="text/javascript" SPK="8833978172476">$("#norton-seal").click(function() {
    var languageCode = $("meta[name='language']").attr("content");
    var top = window.screen.height - 400;
    top = top > 0 ? top/2 : 0;            
    var left = window.screen.width - 500;
    left = left > 0 ? left/2 : 0;
    var screen ="height=400,width=500";
    
