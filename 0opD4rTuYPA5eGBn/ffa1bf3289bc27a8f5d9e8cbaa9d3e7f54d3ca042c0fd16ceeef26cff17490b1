<script>
setTimeout(function(){
	if($("body").find("a[href=tocode]").length>0){
        var qrcodeH =$("body").find("a[href=tocode]").get(0);
       function creatQrcode(rcode){
        var qrcode = new QRCode( qrcodeH, {
            width : 150,
            height : 150
        });
        qrcode.clear();
        qrcode.makeCode(rcode);
        $("body").find("a[href=tocode]").attr("href","javascript:void(0);")
       }    
       creatQrcode(window.location.href);}

},100)
</script>
