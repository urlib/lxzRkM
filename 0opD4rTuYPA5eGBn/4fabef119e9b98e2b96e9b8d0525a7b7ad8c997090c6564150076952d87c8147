		<script type="text/javascript">
					(function(data){
					    if(data){
					      var helper = (function(config){
					              var csstmp ='<link rel="stylesheet" href="url"  crossorigin="anonymous">';
					              var jstmp = "<script type=\"text/javascript\" src=\"url\"><\/script>";
					              var stash = new Array();
					              return {
					                css:function(link){
					                  for(var i in link){
					                      var csslink = csstmp.replace('url',config['commonWebResourcePath']+'/css'+link[i]['href']);
					                      if(link[i]['crossorigin']){
					                          csslink = csslink.replace('anonymous',link[i]['crossorigin']);
					                      }
					                      csslink = csslink.replace('crossorigin="anonymous"','');
					                      if(link[i]['rel']){
					                          csslink = csslink.replace('stylesheet',link[i]['rel']);
					                      }        
					                      $("#mfe-footer-container").append(csslink);
					                    }
					                  },
					                  js:function(obj){
					                    if(obj){
					                      for(var i in obj){
					                        if(obj[i]['defer']){
					                          stash.push(config['commonWebResourcePath']+'/js'+obj[i]['src']);
					                        }else{
					                          var jslink = jstmp.replace('url',config['commonWebResourcePath']+'/js'+obj[i]['src']);
					                          $("#mfe-footer-container").append(jslink);
					                        }
					                      }
					                    }                        
					                  },
					                  callback:function(){
					                    for(var i in stash){
					                       var jslink = jstmp.replace('url',stash[i]);
					                       $("#mfe-footer-container").append(jslink);
					                     }                  
					                  }
					              };
					      })(data['webConfig']);
					      if(data['css']){
					         if(data['css']['standAlone']){
					               helper.css(data['css']['standAlone']);     
					         }
					         if(data['css']['component']){
					            helper.css(data['css']['component']);
					         }
					         if(data['js']['head']){
					           helper.js(data['js']['head']);
					         }
					         if(data['js']['bodyStart']){
					            helper.js(data['js']['bodyStart']);
					         }
					         if(data['js']['bodyEnd']){
					            helper.js(data['js']['bodyEnd']);
					         }
					      }
					      helper.callback();
					      $("#mfe-footer-container .o-mainFooter__processorlogo").html($("#processorLogo_container_by_footer").html()).css('text-align','right');
					    }
