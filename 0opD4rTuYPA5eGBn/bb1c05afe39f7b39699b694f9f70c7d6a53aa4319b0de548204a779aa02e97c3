    <script>
        function recaptchaInitCallback() {
            for (var i = 0; i < document.forms.length; i++) {
                var $f = $(document.forms[i]);
                var $c = $f.find('.g-recaptcha');
                var c;
                if ($c.length > 0) {
                    c = $c[0];
                    var recaptchaId = grecaptcha.render(c, { 'sitekey': $c.data('sitekey') });
                    $f.data('recaptcha-id', recaptchaId);
                }
            }
        }
    </script>
</body>

</html>






