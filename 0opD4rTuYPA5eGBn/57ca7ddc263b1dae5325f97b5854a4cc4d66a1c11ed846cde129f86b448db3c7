    <script>
        (function() {

            var $utilityNavigation = document.querySelector('#c-8e67f050-3065-497e-bece-2b8dd5d5c54a-utility-navigation');
            if (!$utilityNavigation) return;
            window.addEventListener('close.utility.navigation.arm.com', function() {
                $utilityNavigation.close();
            });
            var params = "contextLanguage=en";
            var xhr = new XMLHttpRequest();
            xhr.open('GET', '/api/utility-navigation/a4a6d9bf-b836-4c08-bc06-b8306291bfe9'+"?" + params,true);
            xhr.setRequestHeader('Content-Type', 'application/json');
            xhr.onload = function() {
                if (xhr.status === 200) {
                    try {
                        var items = JSON.parse(xhr.responseText);
                        $utilityNavigation.data = items;
                    }
                    catch (error) {
                        console.error(error);
                    }
                }
            };
            xhr.send();

        })();



    </script>
</div>


                </div>
            </div>

            <script>
                (function () {
                    var isIE11 = !!window.MSInputMethodContext && !!document.documentMode;

                    function setOffset() {
                        var $notificationWrapper = document.querySelector('.c-notifications-wrapper');
                        var computedStyles = getComputedStyle($notificationWrapper);
                        var height = computedStyles.getPropertyValue('height');
                        var $utilityNavigationWrapper = document.querySelector('.c-utility-navigation-wrapper');
                        $utilityNavigationWrapper.style.marginTop = height;
                    }

                    /**
                     * Set a cookie with a specific identifier
                     * @param {string} id
                     * @return {void}
                     */
                    function setCookie(id) {
                        var expiryDate = new Date();
                        expiryDate.setTime(expiryDate.getTime() + (3650 * 24 * 60 * 60 * 1000));
                        document.cookie = id + '=true; expires=' + expiryDate + '; domain=.arm.com; path=/';
                    }

                    /**
                     * Return the cookie value for a specific identifier
                     * @param {string} id
                     * @return {string}
                     */
                    function getCookie(id) {
                        var name = id + '=';
                        var decodedCookie = decodeURIComponent(document.cookie);
                        var ca = decodedCookie.split(';');
                        for (var i = 0; i < ca.length; i++) {
                            var c = ca[i];
                            while (c.charAt(0) == ' ') {
                                c = c.substring(1);
                            }
                            if (c.indexOf(name) == 0) {
                                return c.substring(name.length, c.length);
                            }
                        }
                        return '';
                    }

                    function alertTopBannerOffset(is_close) {
                        var siteWrapper = document.querySelector('.c-site-wrapper');
                        var utilityNavigation = document.querySelector('.c-utility-navigation-sticky-wrapper');
                        var adsAlerts = document.querySelector('.c-alerts.js-alerts');
                        var offsetValue = 'margin-top:' + adsAlerts.offsetHeight + 'px';
                        var isBannerCloseStyle = is_close ? offsetValue : 'margin-top: 0';
                        siteWrapper.setAttribute('style', isBannerCloseStyle);
                        utilityNavigation.setAttribute('style', isBannerCloseStyle);
                        console.log(isBannerCloseStyle);
                      
                    }

                    function alertCloseButtonClicked(evt) {
                        var $component = evt.target;
                        setCookie($component.id);
                        $component.removeEventListener('adsCloseButtonClicked', alertCloseButtonClicked);
                        $component.close();
                        isIE11 && alertTopBannerOffset();
                    }

                    function initialise() {
                        var $alerts = document.querySelectorAll('.js-alerts ads-alert');
                        for (var i = 0; i < $alerts.length; i++) {
                            var $component = $alerts[i];
                            if (!getCookie($component.id)) {
                                $component.addEventListener('adsCloseButtonClicked', alertCloseButtonClicked);
                            } else {
                                $component.remove();
                            }
                        }

                        if (isIE11) {
                            document.querySelector('ads-alert').addEventListener('adsAlertOpened', function (evt) {
                                alertTopBannerOffset(true);
                            });
                            document.querySelector('ads-alert').addEventListener('adsAlertClosed', function (evt) {
                                alertTopBannerOffset(true);
                            });
                            window.addEventListener('resize', function () { alertTopBannerOffset(true) });
                        }
                    }

                    initialise();

                    var $notificationWrapper = document.querySelector('.c-notifications-wrapper');
                    if (!$notificationWrapper) return;
                    var $closeNotification = document.querySelector('.c-notification__close');
                    if (!$closeNotification) return;
                    $closeNotification.onclick = function () {
                        $notificationWrapper.style.display = 'none';
                        var expiryDate = new Date();
                        expiryDate.setTime(expiryDate.getTime() + (3650 * 24 * 60 * 60 * 1000));
                        document.cookie = 'acceptSesameCookie=true; expires=' + expiryDate + '; domain=.arm.com; path=/';
                        if (isIE11) {
                            alertTopBannerOffset(true);
                        }
                    };
                    var acceptCookie = getCookie('acceptSesameCookie');
                    if (acceptCookie) {
                        $notificationWrapper.style.display = 'none';
                    }
                    window.addEventListener('resize', setOffset);
                    setOffset();
                })();
            </script>

            
        </div>

        
