    <script>
        (function() {

            var $utilityNavigation = document.querySelector('#c-8e67f050-3065-497e-bece-2b8dd5d5c54a-utility-navigation');
            if (!$utilityNavigation) return;
            window.addEventListener('close.utility.navigation.arm.com', function() {
                $utilityNavigation.close();
            });
            var params = "contextLanguage=en";
            var xhr = new XMLHttpRequest();
            xhr.open('GET', '/api/utility-navigation/a4a6d9bf-b836-4c08-bc06-b8306291bfe9'+"?" + params,true);
            xhr.setRequestHeader('Content-Type', 'application/json');
            xhr.onload = function() {
                if (xhr.status === 200) {
                    try {
                        var items = JSON.parse(xhr.responseText);
                        $utilityNavigation.data = items;
                    }
                    catch (error) {
                        console.error(error);
                    }
                }
            };
            xhr.send();

        })();



    </script>
