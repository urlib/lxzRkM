  <span class="cm-keyword">public</span> <span class="cm-keyword">static</span> <span class="cm-keyword">final</span> <span class="cm-type">String</span> <span class="cm-variable">PAYMENT_GATEWAY_TOKENIZATION_ID</span> <span class="cm-operator">=</span> <span class="cm-string">"SEU_MERCHANT_ID"</span>;

