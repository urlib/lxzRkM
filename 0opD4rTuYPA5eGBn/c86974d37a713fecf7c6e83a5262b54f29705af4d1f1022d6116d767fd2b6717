                    if (isQrhome) l.Redirect.toProductSupportPage(window["config"]["product"]["ID"]);
                    else $(".mse-psp-info .mse-psp-pc .qrcode-area,.mse-psp-info .mse-psp-mobile .mse-qrcode.icon-qrcode").hide();
                }
            }
        }
    });    
</script>
