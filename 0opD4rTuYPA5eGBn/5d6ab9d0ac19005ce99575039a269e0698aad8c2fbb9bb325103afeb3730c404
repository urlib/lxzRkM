function createEspots(arreglo) {
    for (var i = 0; i < arreglo.length; i++) {

        function copyExist(a) {
            if (a[i].copy != undefined) {
                return a[i].copy
            }
            return ''
        }
        function copyMobileExist(a) {
          if (a[i].copyMobile == false || a[i].copyMobile == undefined) {
              return a[i].copy
          }
          return a[i].copyMobile
      }

        function notFalse(value) {
          if (value != false) {
              return value
          } else {
              return ''
          }
        }


        function internal(url, internalValue){
        if(internalValue === 'internal'){
            return siteroot + url
        }else{
        return url
        }
        }

        var espot = '<div class="item"><a style="display: block;width: 100%;height: 100%;" href="' + notFalse(internal(arreglo[i].link, arreglo[i].internalorExternal)) + '"><div class="copy"><h3>' + notFalse(arreglo[i].title) + '</h3><p class="espots-copy-desktop">' + notFalse(copyExist(arreglo)) + '</p><p class="espots-copy-mobile">' + notFalse(copyMobileExist(arreglo)) + '</p></div></a></div>'
        $('#espots').append(espot);

        var cssEspot = '<style>#espots .item:nth-child(' + (i + 1) + ') {background:url(' + arreglo[i].img + ') center center no-repeat; background-size:cover}</style>'
        $('head').append(cssEspot);
    }
}





function addEspot(position, newTitle, newCopy, mewimg, newLink, newInternal) {

  position = position - 1;
  var newEspot = {

      title: newTitle,
      copy: newCopy,
      internalorExternal: newInternal,
      link: newLink,
      img: mewimg
  }

  espots.splice(position, 0, newEspot);
}

function changeEspot(position, newTitle, newCopy, mewimg, newLink, newInternal) {

  position = position - 1;
  if (position < espots.length) {
      espots[position].title = newTitle;
      espots[position].copy = newCopy;
      espots[position].internalorExternal = newInternal;
      espots[position].link = newLink;
      espots[position].img = mewimg;
  } else {


      var newEspot = {
        title: newTitle,
      copy: newCopy,
    internalorExternal: newInternal,
      link: newLink,
      img: mewimg
      }

      espots.push(newEspot)

  }
}


function removeEspot(position){
    position = position - 1;
    espots.splice( position, 1 );
}





