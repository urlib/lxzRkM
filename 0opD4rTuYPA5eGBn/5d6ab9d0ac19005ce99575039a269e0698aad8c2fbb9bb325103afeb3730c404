$(function() {
    $('.only-text').on('input', function() {
      this.value = this.value.replace(/[^a-zA-Z]/g, '');
    });
  });


  $('.restrict').on("keypress keyup blur change", function () {
    if ($(this).val().length < 1 && $(this).hasClass('error')) {

        $(this).removeClass('error');
        $(this).next().remove();
    }
});




/* js viejo */
$(window).scroll(function () {
    $('div.mobile-first, div.navv').toggleClass('fixed', $(this).scrollTop() > 25);
});
  $('.navvv').on('click', function (e) {
        if (e.target.classList.contains('navvv')) {
            $('.close-popup').trigger('click')
        }
    })



    $(document).on('click', '.close-popup', function () {
        $(this).closest('.smb-popup').removeClass('open');
        $('.navvv').removeClass('openn')
    });


$('.popup-button').click(function () {
    var $popupButton = $(this);
    var $popup = $(this).next();
    var $allPopups = $('.smb-popup');

    var isOpen = $allPopups.hasClass('open');
    if(isOpen) {
        var openedElement = getOpenedElement($allPopups);
        var clickedElement = $popupButton.attr('id');
        closeAllPopups($allPopups);

        if(openedElement != clickedElement) {
            openPopup($popup);
        }
    } else {
        openPopup($popup);
    }

});

    function openPopup(popup) {
    popup.addClass('open');
    $('.navvv').addClass('openn')
}

function getOpenedElement(el) {
    var openedElement;
    el.each(function () {
        if($(this).hasClass('open')) {
            openedElement = $(this).prev().attr('id');
        }
    });
    return openedElement;
}

function closeAllPopups(popups) {
    popups.removeClass('open');
}


$(document).on('click', '.close-popup', function () {
    $(this).closest('.smb-popup').removeClass('open');
});

$(document).on('click','.drop-menu', function () {
    $('.navv').toggleClass('open');
});

$(".navv #smb2-widget .icons div").hover(function(){
    var content = $(this).data("content");
    $(".navv #smb2-widget .icons").css('display','none');
    $(".navv #smb2-widget .hover div").css('display','none');
    $(".navv #smb2-widget .hover div." + content).css('display','block');
    }, function(){
});

$(".navv #smb2-widget .hover").hover(function(){
    }, function(){
    $(".navv #smb2-widget .icons").css('display','flex');
    $(".navv #smb2-widget .hover div").css('display','none');
});


/* marketo JS */

$(document).ready(function() {


