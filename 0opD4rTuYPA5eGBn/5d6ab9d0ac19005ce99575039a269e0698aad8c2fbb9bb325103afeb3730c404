    var d = document,
        g = d.createElement('script'),
        s = d.getElementsByTagName('script')[0];
    g.type = 'text/javascript';
    g.defer = true;
    g.async = true;
    g.src = document.location.protocol + '//us-library.klarnaservices.com/merchant.js?uci=be0b4039-22cc-4426-95ef-b6ef2a64e0c0&country=US';
    s.parentNode.insertBefore(g, s);
})();

