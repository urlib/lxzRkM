var inicial = 0;
var slidsLength = 1;
var ini = 1;



$(document).ready(function(){


createSlide(sliders)
slidsLength = $(".bullet").length;

var controls1 = $(".bullet:nth-child(1)")
var controls2 = $(".bullet:nth-child(2)")
var controls3 = $(".bullet:nth-child(3)")
var controls4 = $(".bullet:nth-child(4)")
var controls5 = $(".bullet:nth-child(5)")




slide(controls1, 0, 1)
slide(controls2, (100 / slidsLength), 2)
slide(controls3, (100 / slidsLength) * 2, 3)
slide(controls4, (100 / slidsLength) * 3, 4)
slide(controls5, (100 / slidsLength) * 4, 5)
$('.bullet:nth-child(' + 1 + ')').addClass('active')

ini = 100 / slidsLength
var seTtieme;


time()

})


function slide(element, position, location) {
    element.on('click', function () {
        $('.bullet').removeClass("active")
        $(this).addClass("active")
        $("#slider-container li").removeClass('show')
        $("#slider-container li:nth-child("+location+")").addClass('show')
        inicial = location - 1
    })
}


function createSlide(arreglo) {
    var arrayMobile = []
    var arrayDesktop = []
    for (var i = 0; i < arreglo.length; i++) {

        function secondButton(a) {
            if (a[i].linkCopy2 != false && a[i].linkCopy2 != undefined) {
                return '<a href="' + link(arreglo[i].link2, arreglo[i].link2internal) + '" class="buy_now">' + a[i].linkCopy2 + '</a>'
            } else {
                return ''
            }
        }




        function title(a) {
            if (a[i].title != undefined && a[i].title != '') {
                return '<a href="'+ link(a[i].link, a[i].linkinternal)  +'"><h3>' + a[i].title + '</h3></a>'
            } else {
                return ''
            }
        }

        function notFalse(value) {
            if (value != false) {
                return value
            } else {
                return ''
            }
        }

        function link(link, value){

            if (value == 'internal'){
                return siteroot + link
            }else{
                return link
            }
        }


        var slid = '<li><a href="' + notFalse(link(arreglo[i].link, arreglo[i].linkinternal)) + '" class="mobile-link" target="_blank"></a><div class="left-desc"><figure><div class="logo"> <img src="' + notFalse(arreglo[i].logo) + '" alt=""></div> <figcaption><div class="copy-container">' + notFalse(title(arreglo)) + ' <p class="desktop-copy">' + notFalse(arreglo[i].copy) + '</p><p class="mobile-copy">' + notFalse(arreglo[i].copyMobile) + '</p></div><div class="btn-container"><a href="' + notFalse(link(arreglo[i].link, arreglo[i].linkinternal)) + '" class="buy_now">' + notFalse(arreglo[i].linkCopy) + '</a>' + secondButton(arreglo) + '</div></figcaption> </figure> </div></li>'
        $('#slider-container').append(slid);
        $('.controls ul').append('<li class="bullet"></li>');
        var backMobile = '.slider .slider_container li:nth-child(' + (i + 1) + '){background:url(' + arreglo[i].imgMobile + ')}'
        var backDesktop = '.slider .slider_container li:nth-child(' + (i + 1) + '){background:url(' + arreglo[i].imgDesktop + ')}'
        arrayMobile.push(backMobile)
        arrayDesktop.push(backDesktop)
    }

    arrayMobile = arrayMobile.toString()
    arrayDesktop = arrayDesktop.toString()
    arrayMobile = arrayMobile.replace(/,/g, " ")
    arrayDesktop = arrayDesktop.replace(/,/g, " ")
    var backSliders = '' + arrayMobile + '@media (min-width:751px) {' + arrayDesktop + '}';
    $('head').append('<style>' + backSliders + '</style>')
    $("#slider-container li:nth-child(1)").addClass('show')
}


function time() {
    seTtieme = window.setInterval(function () {
        if (inicial < slidsLength) {

            $("#slider-container li").removeClass('show')
            $("#slider-container li:nth-child("+(inicial+1)+")").addClass('show')
            $('.bullet').removeClass('active')
            $('.bullet:nth-child(' + (inicial + 1) + ')').addClass('active')
        } else {
            inicial = 0;

            $("#slider-container li").removeClass('show')
            $("#slider-container li:nth-child(1)").addClass('show')
            $('.bullet').removeClass('active')
            $('.bullet:nth-child(1)').addClass('active')
        }
        inicial++;
    }, 6000);
};


function changeSlider(position, newLogo, newTitle, newCopy, newCopymobile, newLinkCopy, newLink, newLinkCopy2, newLink2, newimgDesktop, mewimgMobile,intExt, intext2) {

    position = position - 1;
    if (position < sliders.length) {
        sliders[position].logo = newLogo;
        sliders[position].title = newTitle;
        sliders[position].copy = newCopy;
        sliders[position].copyMobile = newCopymobile;
        sliders[position].link = newLink;
        sliders[position].linkCopy = newLinkCopy;
        sliders[position].link2 = newLink2;
        sliders[position].linkCopy2 = newLinkCopy2;
        sliders[position].imgDesktop = newimgDesktop;
        sliders[position].imgMobile = mewimgMobile;
        sliders[position].linkinternal = intExt;
        sliders[position].link2internal = intext2;
    } else {


        var newSlide = {
            logo: newLogo,
            title: newTitle,
            copy: newCopy,
            copyMobile: newCopymobile,
            linkinternal: intExt,
            link2internal: intExt2,
            link: newLink,
            linkCopy: newLinkCopy,
            link2: newLink2,
            linkCopy2: newLinkCopy2,
            imgDesktop: newimgDesktop,
            imgMobile: mewimgMobile,

        }
        sliders.push(newSlide)
    }
}

function addSlider(position, newLogo, newTitle, newCopy, newCopymobile, newLinkCopy, newLink, newLinkCopy2, newLink2, newimgDesktop, mewimgMobile,intExt, intext2) {

    position = position - 1;
    var newSlide = {
        logo: newLogo,
        title: newTitle,
        copy: newCopy,
        copyMobile: newCopymobile,
        link: newLink,
        linkCopy: newLinkCopy,
        link2: newLink2,
        linkCopy2: newLinkCopy2,
        imgDesktop: newimgDesktop,
        imgMobile: mewimgMobile,
        linkinternal: intExt,
        link2internal: intext2
    }

    sliders.splice(position, 0, newSlide);
}


function removeSlide(position){
    position = position - 1;

    sliders.splice( position, 1 );

}




