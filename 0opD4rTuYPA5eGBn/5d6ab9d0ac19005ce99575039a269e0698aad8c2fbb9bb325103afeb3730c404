                "emailLastSource": $('form#SMB_get_quote input[name="midassource"]').val()
            });
        });
        $("#SMB_contact_us, #SMB_get_quote").bind("submit", function(e) {
            if ($(this).attr("id") == "SMB_contact_us" && $(this).valid()) {
                e.preventDefault();
                contactForm.submit();
                contactForm.onSuccess(function(values, followUpUrl) {
                    $("#SMB_contact_us_Form").fadeOut(200);
                    $("#SMB_contact_us_Thanks").fadeIn(200);
                    return false;
                });
            } else if ($(this).attr("id") == "SMB_get_quote" && $(this).valid()) {
                e.preventDefault();
                quoteForm.submit();
                quoteForm.onSuccess(function(values, followUpUrl) {
                    $("#SMB_get_quote_Form").fadeOut(200);
                    $("#SMB_get_quote_Thanks").fadeIn(200);
                    return false;
                });
            }
        });
        $("#SMB_contact_us").validate({
       onkeyup: function(element) {
           $(element).valid(); 
        },
            rules: {
                emailAddress: {
                    required: true,
                    email: true
                },
                firstName: "required",
                lastName: "required",
                company: "required",
