$('#view-more').on('click', function(){
    $('.categories-element').css('display','inline-block');
    $(this).css('display', 'none')
})

$('#view-less').on('click', function(){
    $('.categories-element').css('display','inline-block');

var vp = $(window).width();
function scVp(screenVP){
if(screenVP >= 1910){
return 6
}else{
return 5
}
}

for(i = scVp(vp); i <= $('.categories-element').length; i++){

    $('.categories-element:nth-child('+i+')').css('display', 'none')
}

    $(this).css('display', 'none')
    $('#view-more').css('display', 'inline-block')

})



function enableButton( id ,number){
setTimeout(function(){ 
  if( validInput(id) == number){
  $("#" + id + "  input[type=submit]").prop("disabled", false)
  }else{
    $("#" + id + "  input[type=submit]").prop("disabled", true)
  }
}, 500);
}


function validInput(id) {

    var inputsRquired = document.getElementById(id).getElementsByTagName('input')
    var selectRquired = document.getElementById(id).getElementsByTagName('select')
    var inputNumber = 0;
    var selectNumber = 0;

    for (var i = 0; i < inputsRquired.length; i++) {

        if (inputsRquired[i].getAttribute("aria-invalid") == 'false') {
            inputNumber = inputNumber + 1
        }

    }



    for (var i = 0; i < selectRquired.length; i++) {

        if (selectRquired[i].getAttribute("aria-invalid") == 'false') {
            selectNumber = selectNumber + 1
        }

    }
    return (selectNumber + inputNumber)

}


$('input, textarea').on('keyup', function(){

enableButton('contact-us', 7)
enableButton('get-quote', 6)
enableButton('sign-up', 5)
})

$('select').on('click', function(){
enableButton('contact-us', 7)
enableButton('get-quote', 6)
enableButton('sign-up', 5)
 
})


