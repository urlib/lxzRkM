    window.setTimeout(function(){
        var siteRoot = $('meta[name="siteRoot"]').attr('content');
        var html = '<style media="screen">.call-chat{bottom: 45px;}#close-telebanner-bottom:hover{cursor: pointer;}.anls-wkend-bnr a{color: white;}.evg-chat-link a{display: inline-block; background: white; color: #00B0FF; padding: 5px; border-radius: 4px; margin-left: 10px; font-weight: bold;}#close-telebanner-bottom{position: fixed; right: 25px; font-size: 25px; padding: 0 1em;}.mainFooter{margin-bottom: 40px;}.anls-wkend-bnr{background-color: #00B0FF; left: 0; right: 0; color: #fff; text-align: center; height: 40px; display: flex; justify-content: space-around; align-items: center; z-index: 9999; position: fixed; bottom: 0px; width: 100%;}@media screen and (max-width: 1024px){.anls-wkend-bnr{position: relative;}}.anls-wkend-desktop img{margin-left: 20px; height: 20px !important;}.anls-wkend-desktop{font-weight: 400; padding: 20px;}.anls-wkend-desktop strong{font-weight: 900; margin-left: 10px;}.anls-wkend-mobile{display: none; padding: 2px 20px;}@media (max-width: 980px){.anls-wkend-bnr{z-index: 99; margin-top: 8px;}.anls-wkend-desktop{display: none;}.anls-wkend-mobile{display: block; padding: 2px 12px; font-size: 15px;}.anls-wkend-mobile img{left: 15px; position: absolute; transform: translateY(-50%); top: 50%;}#close-telebanner-bottom{right: 0; position: absolute;}.anls-wkend-mobile br{display: none;}}@media (max-width: 580px){.anls-wkend-mobile{width: 70%;}.anls-wkend-mobile br{display: block;}.anls-wkend-bnr{height: 50px; line-height: 1.2;}#hero-business{margin-top: 0 !important;}}@media (max-width: 1179px) and (min-width: 751px){#mainContent .container-smb{margin-top: 0;}}</style><div> <div class="anls-wkend-bnr"><span class="anls-wkend-desktop"><strong>Call our Small Business Specialists at <a href="tel:18664260911">866-426-0911</a></strong> <img src="//static.lenovo.com/na/subseries/sticky-banner/tele-banner/lenovo-subseries-telebanner-phone-icon.png"></span> <span class="anls-wkend-mobile"><img src="//static.lenovo.com/na/subseries/sticky-banner/tele-banner/lenovo-subseries-telebanner-phone-icon.png"><strong>Call our Small Business Specialists at <a href="tel:18664260911">866-426-0911</a></strong></span> <div id="close-telebanner-bottom">&times;</div></div></div>';

        var cookiename = getCookie("callcenterClose");

        if(cookiename !== "active" ) {
            if (window.matchMedia("(min-width: 1024px)").matches){
                $("#mainContent").append(html);
            } else {
                $(".container-smb").before(html);
            }
        }
        $('#close-telebanner-bottom').click(function(){
            $('.anls-wkend-bnr').remove();
            $('.mainFooter').css('margin-bottom','0');
            document.cookie = "callcenterClose=active; expires=0; path=/";
            if (window.matchMedia("(max-width:980px)").matches){
                document.querySelector('#hero-business').style.cssText = "margin-top: 8px !important";
            } else if (window.matchMedia("(min-width:981px)").matches){
                document.querySelector('#hero-business').style.cssText = "margin-top: 0px !important";
            } else if (window.matchMedia("(max-width: 1179px) and (min-width: 751px)").matches){
                document.querySelector('#mainContent .container-smb').style.cssText = "margin-top: 22px;"
            }
        });
    },1000);
});

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') c = c.substring(1);
        if (c.indexOf(name) != -1) return c.substring(name.length, c.length);
    }
    return "";
}

