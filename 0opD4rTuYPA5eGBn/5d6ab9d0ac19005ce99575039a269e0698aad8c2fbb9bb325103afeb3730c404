        var contactForm, quoteForm;
        $('.quote, .contact').bind("click", function() {
            if ($(this).attr("class") == "contact") {
                $("#marketoForm form").attr("id", "mktoForm_1008");
                $('input[name="formid"]').val("1008");
                contactForm = MktoForms2.allForms()[0];
                quoteForm = "";
            } else if ($(this).attr("class") == "quote") {
                $("#marketoForm form").attr("id", "mktoForm_1009");
                $('input[name="formid"]').val("1009");
                quoteForm = MktoForms2.allForms()[0];
                contactForm = "";
            }
        });
        $('form#SMB_contact_us input[type="text"], form#SMB_contact_us select, form#SMB_contact_us textarea').bind("change", function() {
            contactForm.vals({
                "FirstName": $('form#SMB_contact_us input[name="firstName"]').val(),
                "LastName": $('form#SMB_contact_us input[name="lastName"]').val(),
                "Company": $('form#SMB_contact_us input[name="company"]').val(),
                "Email": $('form#SMB_contact_us input[name="emailAddress"]').val(),
                "Country": $('form#SMB_contact_us select[name="country"]').val(),
                "contactMeReason": $('form#SMB_contact_us select[name="contactReason1"]').val(),
                "contactMeComment": $('form#SMB_contact_us textarea[name="comments"]').val(),
                "Phone": $('form#SMB_contact_us input[name="busPhone"]').val(),
                "leadSourceLast": $('form#SMB_contact_us input[name="leadSourceMostRecent1"]').val(),
                "emailLastSource": $('form#SMB_contact_us input[name="midassource"]').val()
            });
        });
        $('form#SMB_get_quote input[type="text"], form#SMB_get_quote select, form#SMB_get_quote textarea').bind("change", function() {
            quoteForm.vals({
                "FirstName": $('form#SMB_get_quote input[name="firstName"]').val(),
                "LastName": $('form#SMB_get_quote input[name="lastName"]').val(),
                "Company": $('form#SMB_get_quote input[name="company"]').val(),
