<script type="text/javascript">
    if (typeof msApp == null || typeof msApp === "undefined"){
        var msApp = {};
    }
    msApp.commonWebResourcePath = "//static.lenovo.com/fea";
    msApp.customerHost = "https://www.lenovo.com";
    msApp.contextPath = "";
    msApp.storeRootPath = "";
    msApp.country = "";
    msApp.orgLanguage = "";
    msApp.language =  "";
    msApp.previewCmsHost = "http://global-cms.prod.online.lenovo.com";
    msApp.storename = "";
    msApp.storeCode = "";
    msApp.storeType = "";

    var webRootlocation = msApp.storeRootPath;

