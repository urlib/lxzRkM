    <script>
        // 页面有种分页
        $(function () {
            var leftIcon = $('.service .left-icon');
            var rightIcon = $('.service .right-icon');
            var curCount = $('.service .cur-page-count span').eq(0);
            var amountCount = $('.service .cur-page-count span').eq(1);

            var templates = $('.service .multi-page');
            var tempLength = templates.length;
            amountCount.text(tempLength);
            var pageIndex = 0;
            var isComplete = true;
            leftIcon.click(function () {
                if(!isComplete) return false;
                if(--pageIndex >= 0) {
                    isComplete = false;
                    $('.multi-page.active').fadeOut(200, function () {
                        $(this).removeClass('active');
                        templates.eq(pageIndex).fadeIn(400, function () {
                            templates.eq(pageIndex).addClass('active');
                            isComplete = true;
                            curCount.text(pageIndex + 1);
                            iconColorCtroller();
                        })
                    });
                } else pageIndex = 0;
            })
            rightIcon.click(function () {
                if(!isComplete) return false;
                if(++pageIndex < tempLength) {
                    isComplete = false;
                    $('.multi-page.active').fadeOut(200, function () {
                        $(this).removeClass('active');
                        templates.eq(pageIndex).fadeIn(400, function () {
                            templates.eq(pageIndex).addClass('active');
                            isComplete = true;
                            curCount.text(pageIndex + 1);
                            iconColorCtroller();
                        })
                    });
                } else pageIndex = tempLength - 1;
            })

            function iconColorCtroller () {
                if(tempLength == 1) {
                    leftIcon.addClass('non');
                    rightIcon.addClass('non');
                } else {
                    if(pageIndex == 0) {
                        leftIcon.addClass('non');
                        rightIcon.removeClass('non');
                    } else if(pageIndex == tempLength - 1) {
                        leftIcon.removeClass('non');
                        rightIcon.addClass('non');
                    } else {
                        leftIcon.removeClass('non');
                        rightIcon.removeClass('non');
                    }
                }

            }
            iconColorCtroller();
        })
    </script>
</body>

</html>
