        <script>
            function QQCHAT() {
                $('#B2B_QQ_CHAT').click();
            }
            $(function () {
                $("#QQCHAT").click(function () {
                    QQCHAT();
                });
            });
        </script>
        <script>
            $(function() {
                var suspension = $("#suspension"),
                    suspension_top = $("#top1"),//top1顶部id，上线时要替换
                    JWindowwidth = $(window).width();
                s_t_value = suspension_top.length > 0 ? 50 : "";
                suspension.show();
                if(JWindowwidth > 2366){
                    suspension.addClass("slide-small");
                }else{
                    suspension.addClass("slide-small");
                }
        
                // 右侧悬浮移上去效果
                var jSlideSmall = $(".slide-small-1"),
                    jSlideLinkA = jSlideSmall.find(".slide-link-a a");
                jSlideLinkA.on("mouseenter",function(){
                    var _this = $(this);
                    _this.addClass("a-active");
                    _this.siblings("a").removeClass("a-active");
                    _this.parent().parent().addClass("active");
        
                    if($(this).index()==0){
                        _this.parent().next().find("a").stop().animate({"margin-left":"490px"},400)
                    }else{
        
                        _this.parent().next().find("a").stop().animate({"margin-left":"490px"},400)
                    }
                    if(_this.index()==3){
                        _this.parent().next().show().find("a").eq(_this.index()).stop().animate({"margin-left":"360px"},400)
                    }else{
                        _this.parent().next().show().find("a").eq(_this.index()).stop().animate({"margin-left":"370px"},400);
                    }
                });
        
                // 鼠标移开效果
                jSlideSmall.on("mouseleave",function(){
                    var _this = $(this);
                    jSlideLinkA.removeClass("a-active");
                    _this.find(".slide-link-active-a a").stop().animate({"margin-left":"490px"},300,function(){
                        _this.removeClass("active");
                        _this.find(".slide-link-active-a").hide();
                    })
                })
        
                $(".animate1").on('click', function() {
                    datalink($(this))
                });
                function datalink(e) {
                    var id;
                    var top;
                    id = $(e).attr('data-href');
                    top = $("#" + id).offset().top;
                    $('body,html').stop().animate({
                            "scrollTop": top
                        },
                        600);
                }
            });
        </script>
