            if (timers.hasOwnProperty(key)) {
                params["event"] = 'timer' + timers[key];
                params["eventid"] = (new Date).getTime();
                pixel = buildPixel(params);

                trackTimer(pixel, timers[key] * 1000);
            }
        }
    }

