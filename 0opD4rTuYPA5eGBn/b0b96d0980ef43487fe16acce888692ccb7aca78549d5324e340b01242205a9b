<script type="text/javascript">
    passport.init({"ticket":"5e9b6d3d-4500-47fc-b32b-f2b4a1230fd3","login":loginsuccess,"logout":logoutsuccess});
    jQuery(function(){
        action = window.location.pathname;
        jQuery.cookie('st_source_url', action);
    })
    function loginsuccess() {
        //登陆成功数据录入
        jQuery.ajax({
            type: "GET",
            cache: false,
            dataType: 'json',
            url: '//v4.club.lenovo.com.cn/checkCerpregPassport',
            data: "cerpregpassport="+jQuery.cookie('cerpreg-passport'),
            success: function(data){
                if (typeof(data) == 'undefined') {
                    var data = {
                        'status': 'error'
                    };
                }
                if (data.status == 'ok') {
                    window.location = 'https://club.lenovo.com.cn'+action;
                } else if (data.status == 'new') {
                    window.location = 'https://club.lenovo.com.cn';
                } else if (data.status == 'error') {
                    //alert('st error');
                    //window.history.go(0);
                }
            }
        })
    }
    //退出登录
    function logoutsuccess() {
        window.location = "https://club.lenovo.com.cn"+action;
    }
    //注册
    function regpassport(){
        window.location = "https://reg.lenovo.com.cn/auth/v1/regist?ru=https://club.lenovo.com.cn"+action+"&ticket=5e9b6d3d-4500-47fc-b32b-f2b4a1230fd3";
    }
</script>
