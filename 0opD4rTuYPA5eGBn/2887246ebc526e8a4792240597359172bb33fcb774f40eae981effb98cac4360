	<script>
		// important!
		// 此语句必须放在script标签第一句执行 确保元素overflow值被改变
		$('.hd-wrap-20181029 .hd-nav').css('overflow', 'visible');

		// 当前页面是否出现悬浮层
		window.isNeedSuspendedLayer = false;

		// 当每次页面分辨率改变时，调用此函数
		function putInOrNot() {
			// 无论何种情况，先将所有栏目显示出来
			navItemLis.show();
			// 箭头单元默认隐藏
			navLastLi.hide();
			// 悬浮层内所有元素默认隐藏
			suspendedLayerChildren.hide();
			// 默认将栏目父级元素宽度恢复为初始值。避免当屏幕距离足够宽，程序未执行宽度计算时造成的宽度指定错误
			navHccUl.css('width', 'auto');
			// IE8及以下浏览器css-calc计算不生效，使用js计算元素宽度
			IE8NavWidthCalc();

			// navHccUl.width($(this).parent());

			var visibleRange = navHdNav.width() - 246 - 210 - 17 - allItemsWidth;
			if (visibleRange > 0) {
				window.isNeedSuspendedLayer = false;
				navLastLi.hide();
			} else {
				window.isNeedSuspendedLayer = true;
				var accumulation = 0;
				var actualRange = navItems.width() - 53;
				navHccUl.width(actualRange);
				// navLastLi.css('left', actualRange - 24);

				var isHiddenStart = false;

				navItemLis.each(function (i) {
					if (!isHiddenStart) {
						if ($(this).position().top >= 50) {
							$(this).hide();
							suspendedLayerChildren.eq(i).show();
							isHiddenStart = true;
							navLastLi.show();
						}
					} else {
						$(this).hide();
						suspendedLayerChildren.eq(i).show();
					}
				});

				var lastVisibleLi = $('.hd-cc-li:not(.last-li):hidden').eq(0).prev();
				if (lastVisibleLi.length)
					navLastLi.css('left', lastVisibleLi.position().left + lastVisibleLi.outerWidth(true));
			}
		}

		// 当距离不够时，栏目右侧出现箭头，鼠标滑过时浮现未显示出来的图标
		var navItems = $('.hd-config-container');
		var navHdNav = $('.hd-nav');
		var _hdccli = $('.hd-cc-li');
		var navLastItem = _hdccli.eq(_hdccli.length - 1);
		var navLastLi = $('.hd-cc-li.last-li');
		var navItemLis = $('.hd-cc-li:not(.last-li)');
		var suspendedLayer = $('.suspended-layer');
		var navHccUl = $('.hcc-ul');
		var allItemsWidth = 0;

		// 首次加载将所有的hd-cc-li变为<a> 放入到suspened容器内
		navItemLis.each(function () {
			var curAnchor = $(this).find('.hd-cc-li-title > a');
			suspendedLayer.append($('<a style="display: none;" href="' + curAnchor.attr('href') + '">' + curAnchor.text() + '</a>'));
			allItemsWidth += $(this).outerWidth(true);
		});
		var suspendedLayerChildren = suspendedLayer.children('a');

		// IE8不支持css calc计算
		function IE8NavWidthCalc() {
			if (isIE7 || isIE8) navItems.width(navHdNav.width() - 246 - 17);
		}
		IE8NavWidthCalc();
		putInOrNot();

		$(window).resize(putInOrNot);
	</script>
	<script type="text/javascript" src="//m2.lefile.cn/lenovo_pc_search_common/js/lenovo_search_header.min.js"></script>


</div>
    </div>
