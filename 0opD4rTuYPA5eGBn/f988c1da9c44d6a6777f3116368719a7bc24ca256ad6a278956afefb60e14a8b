	<script src="//js1cdn.clubstatic.lenovo.com.cn/pc3/js/club_v4/pc/lib/sea.js?b0e1knhbloxikl0c"></script>
    <script src="//js1cdn.clubstatic.lenovo.com.cn/pc3/js/club_v4/pc/sea_config.js?b0e1knhbloxikl0c"></script>
    <script type="text/javascript" src="//js1cdn.clubstatic.lenovo.com.cn/pc3/js/club_v4/pc/pages/index.js?b0e1knhbloxikl0c"></script>
    <script>
        //获取用户uid
        function getuid() {
            var lenovoid  = "";
            var uid = $.cookie('LA_C_C_Id') ? $.cookie('LA_C_C_Id') : '';
            if(lenovoid != ''){
                return lenovoid; exit();
            }
            if(uid != '' ){
                return uid; exit();
            }
            return '';
        }
        //获取推荐帖子
        function get_index_forum_post(uid) {
            jQuery.ajax({
                type: "GET",
                cache: false,
                data : {uid:uid},
                dataType: 'json',
                url: 'https://club.lenovo.com.cn/homepagepost',
                success: function(data){
                    if (data.code == 100000) {
                        jQuery(".recommendList").html(data.data);
                    }else{
                        //jQuery(".topforumlist").hide();
                    }
                }
            })
        }
        //获取用户id
        function get_data_good(){
          var get_timeout = setInterval(function(){
              uid = getuid();
                if(uid == '' ){
                    get_data_good();
                }else{
                    clearInterval(get_timeout);
                    get_index_forum_post(uid);
                    homepagegoods(uid);
                }
            },1500);
        }
        //获取推荐产品
        function homepagegoods(uid) {
            jQuery.ajax({
                type: "GET",
                cache: false,
                dataType: 'json',
                data : {uid:uid},
                url: 'https://club.lenovo.com.cn/homepagegoods',
                success: function(data){
                    if(data.html != ''){
                        jQuery("#swipers .bd ul").html('');
                        jQuery("#swipers .bd ul").append(data.html);
                        jQuery("#html_on").html('');
                        jQuery("#html_on").append(data.html_on);
                        jQuery("#swipers").slide({mainCell:".bd ul",autoPlay:true, trigger:'click'});
                    }else{
                        // jQuery(".topgoodslist").hide();
                    }

                }
            })
        }
        jQuery(function(){
            get_data_good();
            //判断是否首次打开页面
            if(document.cookie.indexOf("times=true")==-1){
                jQuery('#welcome_page').show();
                var t=new Date(new Date().getTime()+1000*60*60*24*30);
                document.cookie="times=true; expires="+t.toGMTString();
            }else{
                jQuery('#welcome_page').hide();
            }
            jQuery('.close_btn').click(function(){
                jQuery('#welcome_page').hide();
            })


        });

        //点击分享
        function share(tid) {
            var threadUrl = "https://mclub.lenovo.com.cn/thread-" + tid + "-1-1.html";
            $.get('share', { 'threadUrl': threadUrl }, function (data) {
                if (data.code == 100000) {
                    $("#share").show();
                    $("#two_M").html(data.data);
                } else {
                    return false;
                }
            }, 'json');
        }

        //关闭分享
        $('.share_close_btn').click(function(){
            $("#share").hide();
        })

        //发表按钮
        jQuery(document).ready(function(){
            var  toptopbox= jQuery(window).height();
            if(toptopbox>1184){
                jQuery('.pcx_toptop1').show();
            }
        });
        jQuery(window).scroll(function(){
            var  toptopbox= jQuery('.take_toptopbox1').offset().top;
            if(toptopbox>1165){
                jQuery('.pcx_toptop1').show();
            }else{
                jQuery('.pcx_toptop1').hide();
            }
        });
        //返回顶部
        var ScrollToTop=ScrollToTop||{
            setup:function(){
                var a=jQuery(window).height()/2;
                jQuery(window).scroll(function(){
                    (window.innerWidth?window.pageYOffset:document.documentElement.scrollTop)>=a?jQuery("#ScrollToTop").removeClass("Offscreen"):jQuery("#ScrollToTop").addClass("Offscreen")
                });
                jQuery("#ScrollToTop").click(function(){
                    jQuery("html, body").animate({scrollTop:"0px"},400);
                    return false
                })
            }
        };
        jQuery(document).ready(function(){
            ScrollToTop.setup();
        });
    </script>

    <!--发帖相关js-->
    <script type="text/javascript">
        //发表帖子
        $('#publish').click(function(){
            $(".newpost_mark_wrap").show();
        });

        //关闭发帖
        $("#closeplate").live("click",function(){
            $(".newpost_mark_wrap").hide();
        });

        //点击一级菜单
        $("#firstclass").live("click",function(e){
            $("#firstclass li").removeClass("newpost_inallone_hili");
            $(e.target).parent("li").addClass("newpost_inallone_hili");
            fid=e.target.id;
            var htmls="";
            $.each(JSON.parse($CONFIG.forumList),function(n,value){
                if(fid==n){
                    $.each(value.children,function(a,b){
                        htmls+="<li><a href=\"javascript:void(0);\" id='"+b.fid+"'>"+b.name+"</a></li>";
                    });
                }
            });
            $("#secondclass").html(htmls);

            //显示面包屑
            $(".newpost_markbox ul li").not(":first").remove();
            var fname = $("#firstclass .newpost_inallone_hili").text();
            var ulhtml = "<li>></li><li><a href='/forum-g"+fid+".html'>"+fname+"</a></li>";
            $(".newpost_markbox ul").append(ulhtml);
        });

        //点击二级菜单
        $("#secondclass").live("click",function(e){
            $("#secondclass li").removeClass("newpost_inallone_hili");
            $(e.target).parent("li").addClass("newpost_inallone_hili");
            sid = e.target.id;

            //显示面包屑
            $(".newpost_markbox ul li:gt(2)").remove();
            sname = $("#secondclass .newpost_inallone_hili a").text();
            var ulhtml = "<li>></li><li><a href='/forum-"+sid+"-1.html'>"+sname+"</a></li>";
            $(".newpost_markbox ul").append(ulhtml);
        });

        //弹窗点击发新帖
        $(".newpost_markbox_btn span a").live("click",function(){
            forum_id = $(".newpost_markbox_inallone #secondclass .newpost_inallone_hili a").attr('id');
            if(forum_id){
                var link = "/forum.php?mod=post&action=newthread&fid=" + forum_id;
                $(this).attr('href', link);
            }
        });
    </script>
