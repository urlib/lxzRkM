var acc_siteroot = $('meta[name="siteRoot"]').attr("content").toLowerCase();
var language_code = $("meta[name=language]").attr("content").toLowerCase();
var accLabel;
if (acc_URL.includes('/accessories-and-monitors') && $('ol').hasClass("facetsSearchListing")) {
	switch (language_code)
	{
        case "de":
            accLabel = "SIE SUCHEN KOMPATIBLES ZUBEHÖR FÜR IHREN PC? HIER WERDEN SIE FÜNDIG >";
            break;
        case "fr":
