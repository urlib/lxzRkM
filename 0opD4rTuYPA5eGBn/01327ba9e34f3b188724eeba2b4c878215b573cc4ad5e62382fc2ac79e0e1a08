            break;
        case "no":
            accLabel = "PÅ UTKIKK ETTER KOMPATIBELT TILBEHØR TIL PC-EN? SE HER >";
            break;
        case "fi":
            accLabel = "ETSITKÖ TIETOKONEESI KANSSA YHTEENSOPIVIA LISÄVARUSTEITA? LÖYDÄT NE TÄÄLTÄ >";
            break;
        case "sv":
            accLabel = "LETAR DU EFTER KOMPATIBLA TILLBEHÖR TILL DIN DATOR? TITTA HÄR >";
            break;
        case "da":
            accLabel = "LEDER DU EFTER KOMPATIBELT TILBEHØR TIL DIN PC? FIND DET HER >";
            break;
        case "pt":
            accLabel = "PROCURA ACESSÓRIOS COMPATÍVEIS COM O SEU PC? ENCONTRE AQUI >";
            break;
        default:
            accLabel = "LOOKING FOR COMPATIBLE ACCESSORIES TO YOUR PC? FIND HERE >";
    }
    $('.mainContent.accessoriesRedesignPage').before('<div class="accessories-skinny-banner"><a href="'+ acc_siteroot +'/compatibilityFinder" target="_blank" class="accessories-skinny-banner-link">'+ accLabel +'</a></div><style>.accessories-skinny-banner,.accessories-skinny-banner-link{width: 100%;display: table;} .accessories-skinny-banner-link{color: #fff;background:url(//static.lenovo.com/we/images/site/offer-bg-1.jpg) no-repeat;text-align: center;font-weight: bold;font-size: 1.2em;display: table-cell;vertical-align: middle;height:70px;} @media screen and (max-width: 48em) {.accessories-skinny-banner-link{font-size:1em;}} @media screen and (max-width: 30em) {.accessories-skinny-banner-link{font-size:0.8em;}}</style>');
}</script>
